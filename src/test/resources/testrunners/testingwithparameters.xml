<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Regression Test" verbose="3">

<test name="Google Test">
		<parameter name="url" value="https://www.google.com/webhp?authuser=1" />
		<parameter name="browser" value="chrome" />
		<classes>
		
			<class name="MyTest.GoogleTest" />
		</classes>
		
	</test>
	
<test name="Opencart Test">
		<parameter name="url" value="https://www.opencart.com/" />
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="MyTest.OpenCart" />
		</classes>
	</test>
	
<test name="Flipkart Test">
		<parameter name="url" value="https://www.flipkart.com/" />
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="MyTest.Flipkart" />
		</classes>
	</test>	


</suite>